{
  "name": "stock-manager",
  "version": "0.7.5",
  "private": true,
  "description": "Stock Manager",
  "scripts": {
    "analyze": "ANALYZE=1 max build",
    "build": "max build",
    "deploy": "pnpm run build && pnpm run gh-pages",
    "dev": "pnpm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "setup": "max setup",
    "lint": "pnpm run lint:js && pnpm run lint:style && pnpm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && pnpm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\"",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "max dev",
    "start:dev": "cross-env UMI_ENV=dev MOCK=none max dev",
    "start:no-mock": "cross-env MOCK=none max dev",
    "start:pre": "cross-env UMI_ENV=pre max dev",
    "start:test": "cross-env UMI_ENV=test MOCK=none max dev",
    "tsc": "tsc --noEmit",
    "clean": "rimraf dist node_modules/.cache",
    "reinstall": "rimraf node_modules pnpm-lock.yaml && pnpm install",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,less,md}\"",
    "update-browserslist": "pnpm exec update-browserslist-db@latest"
  },
  "lint-staged": {
    "**/*.less": "stylelint",
    "**/*.{js,jsx,ts,tsx}": "pnpm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "Chrome >= 90",
    "Firefox >= 88",
    "Safari >= 14",
    "Edge >= 90",
    "not dead"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/pro-card": "^2.10.0",
    "@ant-design/pro-form": "^2.32.0",
    "@ant-design/pro-layout": "^7.22.7",
    "@umijs/max": "^4.5.3",
    "antd": "^5.28.0",
    "chinese-days": "^1.5.4",
    "dayjs": "^1.11.19",
    "querystring-es3": "^0.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/webpack": "^5.28.5",
    "@umijs/lint": "^4.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "gh-pages": "^6.3.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "stylelint": "^16.25.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}